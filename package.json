{
  "name": "fictional",
  "description": "Generate fake data deterministically from a given input",
  "version": "0.4.5",
  "type": "commonjs",
  "main": "index.js",
  "exports": {
    "import": "./index.mjs",
    "require": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/oftherivier/fictional.git"
  },
  "bugs": {
    "url": "https://github.com/oftherivier/fictional/issues"
  },
  "homepage": "https://github.com/oftherivier/fictional",
  "author": "Justin van der Merwe",
  "license": "MIT",
  "files": [
    "*.js",
    "utils/*.js",
    "*.mjs",
    "dist",
    "index.d.ts"
  ],
  "oftherivier": {
    "type": "es5",
    "src": "index.js"
  },
  "renovate": {
    "extends": [
      "github>oftherivier/tools"
    ]
  },
  "scripts": {
    "build": "otr build",
    "lint": "otr lint",
    "format": "otr format",
    "test": "otr test",
    "release": "otr release",
    "typetest": "otr typetest",
    "checks": "yarn lint && yarn typetest && yarn test"
  },
  "devDependencies": {
    "@oftherivier/tools": "0.1.41"
  },
  "dependencies": {
    "fast-json-stable-stringify": "^2.1.0",
    "string-hash": "^1.1.3"
  },
  "keywords": [
    "deterministic",
    "fake",
    "data",
    "generator",
    "fictional",
    "fixtures",
    "unit",
    "testing"
  ]
}
